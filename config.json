{
    "appium": {
        "server_url": "http://localhost:4723",
        "auto_start": true,
        "timeout": 60
    },
    "android": {
        "auto_connect": true,
        "preferred_device": null,
        "default_app_package": "",
        "default_app_activity": ""
    },
    "ios": {
        "auto_connect": true,
        "preferred_device": null,
        "default_bundle_id": ""
    },
    "logging": {
        "level": "INFO",
        "enable_detailed_logs": false
    },
    "performance": {
        "use_json_source": true,
        "ignore_unimportant_views": true
    },
    "llm": {
        "model": "gpt-4o",
        "max_tokens": 512,
        "temperature": 0.2,
        "system_prompt": "You are a helpful assistant for mobile automation.",
        "api_base": null
    }
}
